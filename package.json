{
  "name": "universal-backend-engine",
  "version": "1.0.0",
  "description": "A powerful, flexible backend framework supporting multiple API types, databases, and advanced features",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\"",
    "build": "babel src -d dist",
    "security-check": "snyk test",
    "docs": "jsdoc -c jsdoc.json",
    "test:unit": "jest --testPathPattern=src/tests/unit",
    "test:integration": "jest --testPathPattern=src/tests/integration",
    "test:coverage": "jest --coverage",
    "test:performance": "node src/tests/performance/api.performance.js",
    "test:load": "artillery run src/tests/load/load-test.yml",
    "security:scan": "node src/tests/security/security-scan.js",
    "ci": "npm run lint && npm run test:unit && npm run test:integration && npm run test:coverage && npm run security:scan"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "backend",
    "api",
    "rest",
    "graphql",
    "websocket",
    "grpc",
    "multi-tenant",
    "serverless"
  ],
  "author": "Teck",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/schema": "^10.0.0",
    "@prisma/client": "^6.5.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.7.0",
    "graphql": "^16.8.1",
    "helmet": "^7.2.0",
    "i18next": "^24.2.3",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "mysql2": "^3.6.1",
    "openai": "^4.89.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.7.0",
    "pg": "^8.14.1",
    "prisma": "^5.4.2",
    "rate-limit-redis": "^3.0.2",
    "redis": "^4.6.10",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
